// Spawn Bias (for items that do replace)
OptionValue "SpawnBias" {
	-1,    "Disabled"
	 0,    "Replace All"
	 2,    "1 in 3"
	 4,    "1 in 5"
	 9,    "1 in 10"
	 14,   "1 in 15"
	 19,   "1 in 20"
	 24,   "1 in 25"
	 49,   "1 in 50"
	 99,   "1 in 100"
	 149,  "1 in 150"
	 199,  "1 in 200"
}

// Pistols

OptionMenu "majesticSpawning" {
    Title "$MENU_MAJESTIC_TITLE"
    StaticText ""

   	StaticText "$MENU_NOTICE", "Yellow"
    StaticText "$MENU_SPAWNRATE_TEXT1", "White"
    StaticText "$MENU_SPAWNRATE_TEXT2", "White"
    StaticText ""

	StaticText "$MENU_WEPSPAWNOPTIONS", "Yellow"
    StaticText "$MENU_MAJESTIC_PISTOL_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "majestic_pistol_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar majestic_pistol_spawn_bias"
    StaticText "$MENU_MAJESTIC_HUNTER_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "majestic_hunter_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar majestic_hunter_spawn_bias"
    StaticText "$MENU_MAJESTIC_SLAYER_SPAWNTEXT", "White"
	Option "$MENU_WEPSPAWNRATE", "majestic_slayer_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETWEP", "resetcvar majestic_slayer_spawn_bias"
 	StaticText ""

	StaticText "$MENU_MAJESTIC_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "majestic_allowBackpacks", "OnOff"
 	StaticText ""

	StaticText "$MENU_MAGSPAWNOPTIONS", "Yellow"
    StaticText "$MENU_MAJESTICMAG_CLIPMAG_SPAWNTEXT", "White"
	Option "$MENU_MAGSPAWNRATE", "majesticmag_clipmag_spawn_bias", "SpawnBias"
	SafeCommand "$MENU_RESETMAG", "resetcvar majesticmag_clipmag_spawn_bias"
    StaticText ""

	StaticText "$MENU_MAJESTICMAG_BACKPACKS", "White"
	Option "$MENU_BACKPACKSALLOWED", "majesticmag_allowBackpacks", "OnOff"
 	StaticText ""

	StaticText "$MENU_PERSISTENCYOPTIONS", "Yellow"
    StaticText "$MENU_PERSISTENCY_TEXT", "White"
	Option "$MENU_WEPPERSISTENCY", "majestic_persistent_spawning", "OnOff"
    Option "$MENU_MAJESTICMAG_PERSISTENCY", "majesticmag_persistent_spawning", "OnOff"
	SafeCommand "$MENU_RESETPERSISTENCY", "resetcvar majestic_persistent_spawning; majesticmag_persistent_spawning"
 	StaticText ""
   	StaticText ""

	StaticText "$MENU_RESETOPTIONS", "Yellow"
	SafeCommand "$MENU_RESETALLOPTIONS", "majestic_reset_cvars"
}

